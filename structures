#include <stdio.h>
#include <string.h>
struct Person {
  char name[50];
  int areaNo;
  float salary;
} person1;

int main() {
  strcpy(person1.name, "MARY");
  person1.areaNo=19;
  person1. salary = 50000;
  printf("Name: %s\n", person1.name);
  printf("Area No.: %d\n", person1.areaNo);
  printf("Salary: %.2f", person1.salary);

  return 0;
}
